{
  "tabWidth": 2,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleAttributePerLine": true,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "printWidth": 100,
  "tailwindConfig": "./tailwind.config.js",
  "pluginSearchDirs": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
